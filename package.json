    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "next-sitemap": "^4.0.5",
    "prop-types": "^15.8.1",
    "sass": "^1.57.1",
    "typescript": "5.1.6"
